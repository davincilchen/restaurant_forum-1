<h1>Category</h1>

<%= link_to "Restaurants", admin_restaurants_path %>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.id %></td>
        <td><%= category.name %></td>
      </tr>
    <% end %>
</tbody>
</table>
